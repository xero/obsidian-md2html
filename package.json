{
  "name": "obsidian-md2html-plugin",
  "version": "2.1.1",
  "description": "convert markdown to html inside obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "css": "bun run postcss src/css/main.css --no-map -o dist/style.css",
	"js": "uglifyjs ./src/js/obsidian.js -mc toplevel,sequences=false -o ./dist/obsidian.js",
    "web": "node htmlbuild.mjs"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "md2html",
    "converter",
    "markdown to html"
  ],
  "author": "xero",
  "license": "CC0",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "autoprefixer": "^10.4.20",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^9.14.0",
    "obsidian": "latest",
    "obsidian-typings": "^2.3.1-beta.1",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-css-variables": "^0.19.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "tslib": "2.4.0",
    "typescript": "^5.6.3",
    "uglify-js": "^3.19.3",
    "update-browserslist-db": "^1.1.1"
  }
}
