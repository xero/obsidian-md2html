{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "es2021",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"types": [
			"obsidian-typings"
		],
		"lib": [
			"DOM",
			"ES5",
			"ES6",
			"ES7",
			"dom.iterable"
		]
	},
	"include": [
		"**/*.ts"
	]
}
